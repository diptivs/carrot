{
  "name": "pomafocus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.7",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-table": "^6.8.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://pomafocus-dev --delete",


    "predeploy:prod": "REACT_APP_ENV=pomafocusprod npm run build",
    "deploy:prod": "aws s3 sync build/ s3://pomafocus --delete",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id EDXFTJ87VI1Z5 --paths /* && aws cloudfront create-invalidation --distribution-id E2Y7OQG6PNS5SL --paths /*",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
